{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/epharma-api/:path*",
      "destination": "https://epharma-panel.srv557357.hstgr.cloud/:path*"
    },
    {
      "source": "/api_epg/:path*",
      "destination": "https://ttm-backend.srv1079351.hstgr.cloud/api_epg/:path*"
    },
    {
      "source": "/reservations-api/:path*",
      "destination": "https://demo2.srv557357.hstgr.cloud/:path*"
    },
    {
      "source": "/n8n-webhook/:path*",
      "destination": "https://n8n-workflows-cktx.onrender.com/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/epharma-api/:path*",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
}
